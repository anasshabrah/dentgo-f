{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.ts",
      "use": "@vercel/vite"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_SERVER_URL": "@VITE_SERVER_URL",
    "VITE_STRIPE_PUBLISHABLE_KEY": "@VITE_STRIPE_PUBLISHABLE_KEY",
    "VITE_GOOGLE_CLIENT_ID": "@VITE_GOOGLE_CLIENT_ID"
  }
}
